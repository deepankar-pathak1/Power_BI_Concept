"Legal 2 Action Days",MAXX(CURRENTGROUP(),TAT_BO[LEGAL2actiondays]),"NSM 1 Action Days",MAXX(CURRENTGROUP(),TAT_BO[NSM1actiondays]),"NSM 2 Actio Days",maxx(CURRENTGROUP(),TAT_BO[NSM2actiondays]),"NSM 3 Action Days",maxx(CURRENTGROUP(), TAT_BO[NSM3actiondays]),"PSM 1 Action Days",maxx(CURRENTGROUP(),TAT_BO[PSM1actiondays]),"PSM2 Action Days",maxx(CURRENTGROUP(),TAT_BO[PSM2actiondays]),"RLCSM Action Days",MAXX(CURRENTGROUP(),TAT_BO[RLCSMactiondays]),"ZLCSM Action Days",MAXX(CURRENTGROUP(),TAT_BO[ZLCSMactiondays]))
